# Cleaned up example, see original wpa_supplicant.conf for comments.

network={
        ssid="$ESSID_MGT_TLS_EXTRA1"
        key_mgmt=NONE
		scan_ssid=1
}

network={
        ssid="$ESSID_MGT_TLS_EXTRA2"
	    psk="$PASS_MGT_PHISHING_EXTRA2_CLEAR"
	    scan_ssid=1
    	key_mgmt=WPA-PSK
    	proto=WPA2
}


network={
        ssid="$ESSID_MGT_TLS_EXTRA3"
        key_mgmt=NONE
		scan_ssid=1
}

network={
	ssid="$ESSID_MGT_TLS"
	scan_ssid=1
	mode=0
	proto=RSN
	key_mgmt=WPA-EAP
	auth_alg=OPEN
	eap=TLS
    anonymous_identity="$ANON_IDENTITY_MGT_TLS"
	identity="$IDENTITY_MGT_PHISHING"
	ca_cert="/root/certs/ca.crt"
	client_cert="/root/certs/client.crt"
	private_key="/root/certs/client.key"
	private_key_passwd="whatever" 
	ieee80211w=0 # do not use Protected Management Frames (PMF)
}