
# wifi-regional client are well configured and check the CA

network={
    ssid="$ESSID_MGT_RELAY"
    scan_ssid=1
    key_mgmt=WPA-EAP
    eap=PEAP
    anonymous_identity="$ANON_IDENTITY_MGT_RELAY"
    identity="$IDENTITY_MGT_RELAY"
    password="$PASS_MGT_RELAY_CLEAR"
    phase1="peapver=1"
	ca_cert="/root/certs/ca.crt"
    phase2="auth=MSCHAPV2"
    ieee80211w=0
}
