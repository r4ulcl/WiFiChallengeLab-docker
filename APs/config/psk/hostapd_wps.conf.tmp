interface=$WLAN_WPS
driver=nl80211
ssid=$ESSID_WPS
channel=$CHANNEL_WPS
hw_mode=g

# Keep WPSâ€‘friendly security
wpa=2
# WPA2 only
wpa_key_mgmt=WPA-PSK
rsn_pairwise=CCMP
# drop TKIP
wpa_passphrase=$PASS_WPS_CLEAR

### WPS ###
eap_server=1
wps_state=2
ap_setup_locked=0
device_name=QCA Access Point
manufacturer=Qualcomm Atheros
device_type=6-0050F204-1
config_methods=label keypad push_button
ap_pin=12345670

ctrl_interface=/run/hostapd-$WLAN_WPS
ctrl_interface_group=0